	xmlns:x="urn:import:muksihs.ipfs.photogallery.ui">
	<ui:style>
	
	</ui:style>
	<m:MaterialPanel>
		<x:HeaderBlock ui:field="hblock"></x:HeaderBlock>
		<m:MaterialLabel style="margin: 0.2em; margin-right: 0.4em; display: inline;" text="Select images for upload:"></m:MaterialLabel>
